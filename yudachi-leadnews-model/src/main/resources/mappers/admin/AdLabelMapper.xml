    		ad_label
    	where
    		id = #{id}
  	</delete>

	<insert id="insert" parameterType="com.yudachi.model.admin.pojos.AdLabel">
		<selectKey resultType="java.lang.Integer" order="AFTER" keyColumn="id" keyProperty="id">
			select last_insert_id()
		</selectKey>
		insert into
			ad_label (name, type, created_time)
		values
			(#{name}, #{type}, #{createdTime})
	</insert>

	<insert id="insertSelective" parameterType="com.yudachi.model.admin.pojos.AdLabel">
		insert into
			ad_label
		<trim prefix="(" suffix=")" suffixOverrides=",">
			<if test="id != null">
				id,
			</if>
			<if test="name != null">
				name,
			</if>
			<if test="type != null">
				type,
			</if>
			<if test="createdTime != null">
				created_time,
			</if>
		</trim>
		<trim prefix="values (" suffix=")" suffixOverrides=",">
			<if test="id != null">
				#{id},
			</if>
			<if test="name != null">
				#{name},
			</if>
			<if test="type != null">
				#{type},
			</if>
			<if test="createdTime != null">
				#{createdTime},
			</if>
		</trim>
	</insert>

	<update id="updateByPrimaryKeySelective" parameterType="com.yudachi.model.admin.pojos.AdLabel">
		update
			ad_label
		<set>
			<if test="name != null">
				name = #{name},
			</if>
			<if test="type != null">
				type = #{type},
			</if>
			<if test="createdTime != null">
				created_time = #{createdTime},
			</if>
		</set>
		where id = #{id}
	</update>

	<update id="updateByPrimaryKey" parameterType="com.yudachi.model.admin.pojos.AdLabel">
    	update
    		ad_label
